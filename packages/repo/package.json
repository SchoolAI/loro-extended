  },
  "scripts": {
    "build": "tsup",
    "check": "biome check --write .",
    "test": "NO_COLOR=true vitest",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@logtape/logtape": "^1.1.1",
    "@loro-extended/change": "workspace:^",
    "emittery": "^1.0.1",
