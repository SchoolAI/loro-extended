    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "loro-crdt": "^1.10.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "lodash-es": "^4.17.21",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
