{
  "name": "example-todo-sse",
  "private": true,
  "version": "0.2.4",
  "type": "module",
  "scripts": {
    "build:all": "pnpm build:client && pnpm build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.node.json",
    "dev": "concurrently --raw \"npm:dev:server\" \"npm:dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx --watch src/server/server.ts",
    "format": "biome format --write .",
    "serve:client": "vite preview",
    "serve:server": "node dist/src/server/server.js",
    "test:all": "pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.node.json"
  },
  "dependencies": {
    "@logtape/logtape": "^1.1.1",
    "@loro-extended/adapter-leveldb": "workspace:^",
    "@loro-extended/adapter-sse": "workspace:^",
    "@loro-extended/change": "workspace:^",
    "@loro-extended/react": "workspace:^",
    "@loro-extended/repo": "workspace:^",
    "classic-level": "^1.2.0",
    "cors": "^2.8.5",
    "emittery": "^1.0.1",
    "express": "^4.19.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
