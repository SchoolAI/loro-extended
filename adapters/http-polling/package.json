{
  "name": "@loro-extended/adapter-http-polling",
  "version": "2.0.0",
  "description": "HTTP polling network adapter for @loro-extended/repo with long-polling support",
  "author": "Duane Johnson",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./server": {
